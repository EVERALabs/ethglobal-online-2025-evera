# Web3 Authentication Provider
# Options: privy, rainbow, xellar
VITE_AUTH_PROVIDER=privy

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api

# Privy Configuration (if using Privy)
VITE_PRIVY_APP_ID=your_privy_app_id_here

# Rainbow Configuration (if using Rainbow)
VITE_RAINBOW_PROJECT_ID=your_rainbow_project_id_here

# Xellar Configuration (if using Xellar)
VITE_XELLAR_API_KEY=your_xellar_api_key_here

# Network Configuration
VITE_CHAIN_ID=1
VITE_NETWORK_NAME=mainnet

# Development
VITE_ENVIRONMENT=development
